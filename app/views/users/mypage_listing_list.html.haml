.default-container  
  = render 'shared/header'

  %nav.bread-crumbs
    %ul
      %li 
        = link_to 'メルカリ',  root_path, class: "bread-crumbs__link"  
        %i.fa.fa-chevron-right
      %li
        %span.name マイページ

  %main.user-profile
    .profile-main   
      .mypage-tab
        %h2.mypage-tab-head 
          出品した商品
        .mypage-tabs
          %ul.mypage-tabs-left
            %li.mypage-tabs-left__tab
              %h3.tabs-title 出品中
          %ul.mypage-tabs-right
            %li.mypge-tabs-right__tab
              %h3.tabs-title 取引中
          %ul.mypage-tabs-right
            %li.mypge-tabs-right__tab
              %h3.tabs-title 売却済み

        .mypage-tab-content
          %ul.transaction-now
            %li.transaction-not-found 取引中の商品がありません



    .profile-side
      = render 'shared/mypage_nav'
  
  = render 'shared/footer'
  = render 'shared/sell_page_btn'